<div class="course-demo-card container-fluid mt-3">
    <div class="d-sm-flex align-items-center justify-content-between mb-2">

        <nav aria-label="breadcrumb">
            <ol class="breadcrumb" style="background-color:white;">
                <li class="breadcrumb-item"><a href="~/" onclick="">Home</a></li>
                <li class="breadcrumb-item"><a href="~/JoinLiveClass" onclick="">JoinLiveClass</a></li>
                <li class="breadcrumb-item active" aria-current="page">Join classroom</li>
            </ol>
        </nav>
    </div>
    <section class="ftco-intro" style="background-image: url(../Images/bg_5.jpg);" data-stellar-background-ratio="0.5">
        <div class="overlay" style="opacity:0.1;"></div>
        <div class="container">
            <div class="row">
                <div class="col-md-9">
                    <h2 style="color: rgb(0 0 0 / 64%);">@Model.m_strClassroomName</h2>
                    @if (Model.m_strClassroomCharge != "free")
                    {
                        <h4 class="" id="classroomJoiningText">Price <strong>&#8377<span class="" id="classroomJoiningFee">@Model.m_strClassroomCharge</span> |  USD &#36;@Model.m_iPriceForForeignStudents </strong> </h4>
                    }
                    else
                    {
                        <h4 class="text-white" id="classroomJoiningText">Price free  </h4>
                    }
                    @if (Model.m_bIsRegistrationClosed)
                    {
                        <p class="text-gray-200 h6 font-weight-bolder" style="color: rgb(0 0 0 / 51%);" id="classroomCreationDate">Sorry! you can't join this course. Registration has been closed for this course.</p>
                    }
                    else
                    {
                        if (Model.m_strLiveClassStartDate != null && Model.m_strLiveClassStartDate != "")
                        {
                            if (Model.m_bIsClassStarted)
                            {
                                <p class="text-gray-200 h6 font-weight-bolder" style="color: rgb(0 0 0 / 51%);" id="classroomCreationDate">Live class is started from @Model.m_strLiveClassStartDate.</p>
                            }
                            else
                            {
                                <p class="text-gray-200  h6 font-weight-bolder" style="color: rgb(0 0 0 / 51%);" id="classroomCreationDate">Live class will start from @Model.m_strLiveClassStartDate.</p>
                            }
                        }
                        else
                        {

                        }
                        <span class="badge badge-pill badge-light" id="successbadge" style="display:none">Joined Successfully</span>
                        if (Model.m_strClassroomCharge != "free")
                        {
                            <p class="text-danger font-weight-bold">Attend @Model.m_iNoOfDemoClassrooms demo sessions free and pay only after you are satisfied.</p>
                            <button onclick="LaunchDemoModal()" class="btn btn-danger">Book demo class</button>
                        }
                        <h6 class="text-info font-weight-bold" href="@ViewBag.ReturnUrl">Hurry! Few seats available</h6>
                    }
                </div>
                <div class="col-md-3 d-flex align-items-center">

                    <p class="mb-0"><a href="../Checkout?id=@Model.m_llClassroomId" class="btn btn-secondary px-4 py-3">Join the course</a></p>
                </div>


            </div>
        </div>
    </section>

    <div class="course-description mt-4">
        <h2 class="font-weight-bold">About Course</h2>
        <P class="text-gray-600">@Model.m_strClassroomDescription</P>
    </div>
    @if (Model.lsHighlights != null && Model.lsHighlights.Length > 0)
    {
        <div class="course-description ">
            <h2 class="font-weight-bold">Course highlights</h2>
            <ul>
                @for (var i = 0; i < Model.lsHighlights.Length; i++)
                {
                    if (Model.lsHighlights[i].Length > 0)
                    {
                        <li class="p  mb-0 text-gray-700" style="font-size:16px;">@Model.lsHighlights[i].</li>
                    }

                }
            </ul>
            @if (Model.broucherUrl != null&&Model.broucherUrl.Length>0)
            {
                <a href="@Model.broucherUrl" class="btn btn-secondary"><i class="fa fa-download" aria-hidden="true"></i> Download Broucher </a>
            }
             
        </div>
       
    }
    else
    {
        <div class="course-description ">
            <h2 class="font-weight-bold">Course highlights</h2>
            <ul>
                <li class="h5 mb-0 text-gray-800">@Model.m_iNoOfLiveClass + Live classes</li>
                <li class="h5 mb-0 text-gray-800">50+ assignments, quizes and study materials </li>
                <li class="h5 mb-0 text-gray-800">5+ Projects</li>
                <li class="h5 mb-0 text-gray-800">1000 + Students joined</li>
                <li class="h5 mb-0 text-gray-800">Research paper support if needed</li>
            </ul>

        </div>
    }

